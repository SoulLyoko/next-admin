{
  "name": "t3-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm db:deploy && pnpm -F web build",
    "start": "pnpm -F web start",
    "dev": "pnpm db:migrate && pnpm -F web dev",
    "check": "next lint && tsc --noEmit",
    "db:generate": "pnpm -F db db:generate",
    "db:migrate": "pnpm -F db db:migrate",
    "db:deploy": "pnpm -F db db:deploy",
    "db:studio": "pnpm -F db db:studio",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "pnpm -F db generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "pnpm build && pnpm start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "superjson": "^2.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.3.0"
}
